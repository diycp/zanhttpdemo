{
  "name": "zanphp/zanhttpdemo",
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "http://packagist.qima-inc.com"
    }
  },
  "require": {
    "zanphp/zan": "dev-master",
    "zanphp/nova": "dev-master",
    "symfony/console": "3.1",
    "packaged/thrift": "0.9.2.1",
    "psr/log": "1.0.0",
    "nova-service/generic": "1.0.2",
    "nova-service/generic-test": "dev-master",
    "nova-service/pf": "dev-master",
    "nova-service/scrm": "dev-master",
    "nova-service/scrm-base": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Com\\Youzan\\ZanHttpDemo\\": "src",
      "Com\\Youzan\\ZanHttpDemo\\Tests\\": "test",
      "Com\\Youzan\\ZanHttpDemoInit\\": "init"
    },
    "classmap": []
  },
  "autoload-dev": {
    "classmap": []
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "~0.9.2"
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}

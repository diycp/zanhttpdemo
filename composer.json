{
  "name": "zanphp/httpdemo",
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:youzan/zanphp.git"
    },
    {
      "type": "path",
      "url": "nova-sdk/nova-demo/"
    }
  ],
  "require": {
    "zanphp/zan": "2.x-dev",
    "nova-service/nova-demo": "*"
  },
  "autoload": {
    "psr-4": {
      "Com\\Youzan\\ZanHttpDemo\\": "src",
      "Com\\Youzan\\ZanHttpDemoInit\\": "init"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "prefer-stable": false,
  "minimum-stability": "dev"
}
